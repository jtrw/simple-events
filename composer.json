{
  "name": "jtrw/events",
  "type": "library",
  "description": "Events",
  "keywords": [
    "Nil Borodulia",
    "jtrw",
    "Events",
    "Simple Event"
  ],
  "homepage": "https://github.com/jtrw/simple-events",
  "license": "MIT",
  "authors": [
    {
      "name": "Nil",
      "email": "brdnlsrg@gmail.com",
      "homepage": "https://github.com/jtrw",
      "role": "Developer"
    }
  ],
  "require": {
    "php" : ">=7.2.5",
    "symfony/event-dispatcher": "^5.1"
  },
  "require-dev": {
    "phpunit/phpunit" : ">=8.0",
    "squizlabs/php_codesniffer": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "Jtrw\\Events\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Jtrw\\Events\\Tests\\": "tests",
      "Jtrw\\Events\\Tests\\Resources\\": "tests/Resources"
    }
  },
  "scripts": {
    "test": "phpunit",
    "check-style": "phpcs src tests",
    "fix-style": "phpcbf src tests"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
